{
  "name": "scm-survey",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:safe": "cross-env NODE_OPTIONS=--max-old-space-size=4096 next dev --port 3000",
    "dev:fast": "cross-env NODE_OPTIONS=--max-old-space-size=8192 next dev --port 3000",
    "dev:clean": "npm run clean && npm run dev:safe",
    "build": "next build",
    "build:safe": "cross-env NODE_OPTIONS=--max-old-space-size=4096 next build",
    "build:clean": "npm run clean && npm run build:safe",
    "build:force": "npm run clean && cross-env NODE_OPTIONS=--max-old-space-size=8192 next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rimraf .next",
    "clean:cache": "rimraf .next && rimraf node_modules/.cache",
    "clean:all": "rimraf .next && rimraf node_modules",
    "kill:node": "taskkill /f /im node.exe 2>nul || echo No node processes found"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.18.0",
    "@mui/material-nextjs": "^7.3.0",
    "@react-pdf/renderer": "^4.3.0",
    "@types/nodemailer": "^7.0.0",
    "bcryptjs": "^3.0.2",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "next": "^14.2.29",
    "nodemailer": "^7.0.5",
    "pg": "^8.18.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.15.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.991.0",
    "@aws-sdk/client-codebuild": "^3.991.0",
    "@aws-sdk/client-codepipeline": "^3.991.0",
    "@aws-sdk/client-codestar-connections": "^3.991.0",
    "@aws-sdk/client-ec2": "^3.991.0",
    "@aws-sdk/client-ecr": "^3.991.0",
    "@aws-sdk/client-ecs": "^3.991.0",
    "@aws-sdk/client-elastic-load-balancing-v2": "^3.991.0",
    "@aws-sdk/client-iam": "^3.991.0",
    "@aws-sdk/client-s3": "^3.991.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.11.19",
    "@types/pg": "^8.16.0",
    "@types/react": "^18.3.22",
    "@types/react-dom": "^18.3.7",
    "@types/recharts": "^2.0.1",
    "@types/uuid": "^9.0.8",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.17",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.35",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  }
}
